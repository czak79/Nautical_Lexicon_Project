  <teiHeader>
    <fileDesc>
      <titleStmt>
        <title>TEI Customization for the Nautical Lexicon Project</title>
        <editor role="creator">Claire Zak</editor>
      </titleStmt>
      <publicationStmt>
        <p>Publication Information - multilingual glossary of nautical terms</p>
      </publicationStmt>
      <sourceDesc>
        <p>This project is based on 3x5 index cards of glossary information and notes which
          have been transcribed</p>
      </sourceDesc>
    </fileDesc>
  </teiHeader>
  <text>
    <body>
      <schemaSpec ident="NameoftheODDfilehere" start="TEI">
        <moduleRef key="header"/>
        <moduleRef key="core"/>
        <moduleRef key="tei"/>
        <moduleRef key="textstructure" except="div1 div2 div3 div4 div5 div6 div7"/>
        <moduleRef key="namesdates"/>
        <moduleRef key="dictionaries" include="superEntry entry re sense figure standOff bibl orth note ref def xr etym form usg anchor"/>

        <classSpec type="atts" ident="att.canonical" module="tei" mode="change">
          <attList>
            <attDef ident="ref" mode="change">
              <constraintSpec ident="nautical-ref" scheme="schematron">
                <constraint>
                  <sch:rule context="//@ref">
                    <sch:report test="contains(., ' ')" role="error">The Nautical Lexicon Project
                      only allows one value on a @ref attribute.</sch:report>
                  </sch:rule>
                  <sch:rule context="//tei:orth/@ref">
                    <sch:assert test="matches(., concat('http://syriaca.org/person/', '\d+'))"
                      role="error"> The @ref attribute on &lt;persName&gt; must take a term URI
                      which reqires the form 'http://syriaca.org/person/{\d+}' (where {\d+} is a
                      number). </sch:assert>
                  </sch:rule>
                </constraint>
              </constraintSpec>
            </attDef>
          </attList>
        </classSpec>

